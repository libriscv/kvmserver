{
	"filename": "/usr/bin/node",
	"ephemeral": true,
	"concurrency": 2,
	"max_boot_time": 24.0,
	"address_space": 16000,
	"max_memory": 24000,
	"max_request_memory": 512,
	"executable_heap": true,
	"main_arguments": [
		"--single-threaded", "$PWD/node/main.js"
	],
	"current_working_directory": "$PWD/node",
	"allowed_paths": [
		"/dev/urandom",
		"/etc/resolv.conf",
		"/etc/ssl/openssl.cnf",
		"$$PWD",
		{
			"virtual": "/proc/self/maps",
			"real": "$PWD/node/maps.fake"
		}
	]
}